                                        <div class="col-md-3">
                                            <label>
                                                <input value="<%= week[i].value %>" name="isclosed" type="checkbox" <% if( vendorData.vendor_timings.length && vendorData.vendor_timings[i].isClosed) { %> checked <% } %>>  Closed
                                            </label>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <% } %>
