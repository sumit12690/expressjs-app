                                                <% for(var k=0; k < time.length; k++) { %>
                                                    <option value="<%= time[k].value %>" <% if(vendorData.vendor_timings.length && vendorData.vendor_timings[i].endTime == time[k].value) { %> selected <% } %>>
                                                        <%= time[k].name %>
                                                    </option>
                                                    <% } %>
