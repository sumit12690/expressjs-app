<% include partials/header.ejs %>
    <div class="container">
        <div class="row ntop">
            <div class="col-md-12 form-group">
                <a href="/vendors" class="btn btn-primary">Go Back</a>
            </div>
            <div class="mainbox col-md-12">
                <form id="orderform" class="form-horizontal" role="form" method="post">
                    <div class="panel panel-primary">
                        <div class="panel-heading">
                            <div class="panel-title">Vendor Basic Info</div>
                        </div>
                        <div style="padding-top:30px" class="panel-body">
                            <% if(categoryList.length != 0) { %>
                                <div class="form-group">
                                    <label for="name" class="col-md-3 control-label">Vendor Category</label>
                                    <div class="col-md-9">
                                        <select name="cat_id" class="form-control chosen-select category">
                                                <option value=''>Select Category</option>
                                            <% categoryList.forEach(category=> { %>
                                                <option value="<%= category.id %>" <% if(vendorData.business_category.cat_id==category.id) { %>
                                                    selected
                                                    <% } %>
                                                        >
                                                        <%= category.catname %>
                                                </option>
                                                <% })  %>
                                        </select>
                                        <input type="hidden" id="old_subcat_id" name="old_subcat_id" value="<%= vendorData.business_subcategory.cat_id %>">
                                    </div>
                                </div>
                                <% } %>
                                <div class="form-group subcategory_div" style="display: none;">
                                        <label for="name" class="col-md-3 control-label">Vendor SubCategory</label>
                                        <div class="col-md-9">
                                            <select name="subcat_id" class="form-control subcategory_dropdown">
                                                <option value=''>Select SubCategory</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="name" class="col-md-3 control-label">Vendor Name
                                            <span class="text-danger">*</span>
                                        </label>
                                        <div class="col-md-9">
                                            <input type="hidden" class="form-control" id="vendorID" value='<%= vendorData._id %>'>
                                            <input type="text" class="form-control" name="vendor_name" placeholder="Vendor Name" value='<%= vendorData.vendor_name %>'>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="profile_url" class="col-md-3 control-label">Vendor Description
                                            <span class="text-danger">*</span>
                                        </label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="vendor_desc" placeholder="Vendor Description" value='<%= vendorData.vendor_desc %>'>
                                        </div>
                                    </div>
                                    <div id='phone_div'>
                                        <% if(vendorData.vendor_phones.length !==0) {
                                            var count=1;
                                            vendorData.vendor_phones.forEach(vendor_phone => { %>
                                                <div class="form-group first_phone_div">
                                                    <label for="name" class="col-md-3 control-label"><% if(count==1) {  %> Vendor Phone <span class="text-danger">*</span> <% } %></label>
                                                    <div class="col-md-3">
                                                        <input type="text" class="form-control" name="vendor_phone" placeholder="Vendor Phone" value='<%= vendor_phone.phone %>'>
                                                    </div>
                                                    <div class="col-md-4">
                                                        <input name="isOwner" value="<%= count %>" type="checkbox" <% if(vendor_phone.isOwner) { %> checked <% } %>>Owner
                                                        <input name="isManager" value="<%= count %>" type="checkbox" <% if(vendor_phone.isManager) { %> checked <% } %>>Manager
                                                        <input name="isWhatsapp" value="<%= count %>" type="checkbox" class='isWhatsapp phone_checkbox' onClick="calculate(this,'isWhatsapp')" <% if(vendor_phone.isWhatsapp) { %> checked <% } %>>Whatsapp
                                                        <input name="isCall" value="<%= count %>" type="checkbox" class='isCall phone_checkbox' onClick="calculate(this,'isCall',2)" <% if(vendor_phone.isCall) { %> checked <% } %>>Call
                                                        <input name="isSMS" value="<%= count %>" type="checkbox" class='isSMS phone_checkbox' onClick="calculate(this,'isSMS',3)" <% if(vendor_phone.isSMS) { %> checked <% } %>>SMS
                                                    </div>
                                                    <% if(count==1) {  %>
                                                        <div class="col-md-2 add_more_div">
                                                            <button type="button" class="btn primary add add_more">
                                                                <span class="glyphicon glyphicon-plus-sign"></span>
                                                            </button>
                                                        </div>
                                                    <% } else { %>
                                                        <div class="col-md-2 add_more_div">
                                                            <button type="button" class="btn default remove">
                                                                <span class="glyphicon glyphicon-minus-sign"></span>
                                                            </button>
                                                        </div>
                                                    <%  } count++; %>
                                                </div>
                                            <%  })} %>
                                    </div>
                                    <div class="form-group">
                                            <label for="subscription_type" class="col-md-3 control-label">Subscription Type <span class="text-danger">*</span></label>
                                            <div class="col-md-9">
                                                <select class="form-control" name="subscription_type" required>
                                                    <option value='bronze' <% if(vendorData.subscription_type) { if(vendorData.subscription_type=='bronze') { %> selected <% }} %>>Bronze</option>
                                                    <option value='silver' <% if(vendorData.subscription_type) { if(vendorData.subscription_type=='silver') {  %> selected <% }} %>>Silver</option>
                                                    <option value='gold' <% if(vendorData.subscription_type) { if(vendorData.subscription_type=='gold')  { %> selected <% }} %>>Gold</option>
                                                </select>
                                            </div>
                                        </div>
                                    
                    </div>
